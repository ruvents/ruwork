{
    "name": "ruwork/route-optional-prefix",
    "description": "Ruwork Route Optional Prefix",
    "type": "library",
    "keywords": ["ruwork"],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {"name": "Valentin Udaltsov", "email": "udaltsov@ruvents.com"}
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/routing": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/config": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\RouteOptionalPrefix\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
