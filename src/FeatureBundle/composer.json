{
    "name": "ruwork/feature-bundle",
    "description": "Ruwork Feature Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "ruwork"
    ],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/event-dispatcher": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "twig/twig": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\FeatureBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.14-dev"
        }
    }
}
