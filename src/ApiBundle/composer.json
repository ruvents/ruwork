{
    "name": "ruwork/api-bundle",
    "description": "Ruwork API Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "ruwork"
    ],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/annotations": "^1.4",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/routing": "^3.4 || ^4.0",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "symfony/form": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/serializer": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\ApiBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
