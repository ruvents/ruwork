{
    "name": "ruwork/upload-bundle",
    "description": "Ruwork Upload Bundle",
    "type": "symfony-bundle",
    "keywords": ["ruwork"],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {"name": "Valentin Udaltsov", "email": "udaltsov@ruvents.com"}
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/orm": "^2.5",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/form": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/serializer": "^3.4 || ^4.0",
        "symfony/validator": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\UploadBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
