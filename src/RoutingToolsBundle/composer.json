{
    "name": "ruwork/routing-tools-bundle",
    "description": "Ruwork Routing Tools Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "ruwork"
    ],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^3.1",
        "ruwork/bundle-test": "^0.1",
        "ruwork/route-optional-prefix": "^0.1",
        "symfony/property-access": "^3.4 || ^4.0",
        "symfony/twig-bridge": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\RoutingToolsBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
