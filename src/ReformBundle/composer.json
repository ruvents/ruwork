{
    "name": "ruwork/reform-bundle",
    "description": "Ruwork Reform Bundle",
    "type": "symfony-bundle",
    "keywords": ["ruwork"],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {"name": "Valentin Udaltsov", "email": "udaltsov@ruvents.com"}
    ],
    "require": {
        "php": "^7.1.3",
        "ruwork/polyfill-form-dti-bundle": "^0.2",
        "ruwork/reform": "^0.1.7",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^3.1",
        "matthiasnoback/symfony-dependency-injection-test": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\ReformBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
