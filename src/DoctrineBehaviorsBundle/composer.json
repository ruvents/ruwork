{
    "name": "ruwork/doctrine-behaviors-bundle",
    "description": "Ruwork Doctrine Behaviors Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "ruwork"
    ],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.5",
        "ruwork/annotation-tools": "^0.12",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "require-dev": {
        "php": "^7.1.3",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/security": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\DoctrineBehaviorsBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.12-dev"
        }
    }
}
