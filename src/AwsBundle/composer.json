{
    "name": "ruwork/aws-bundle",
    "description": "Ruwork AWS Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "ruwork"
    ],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^3.1",
        "php-http/discovery": "^1.3",
        "php-http/httplug": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\AwsBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.12-dev"
        }
    }
}
