{
    "description": "Ruwork monorepo package.",
    "keywords": ["ruwork"],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {"name": "Valentin Udaltsov", "email": "udaltsov@ruvents.com"}
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "wikimedia/composer-merge-plugin": "^1.4",
        "symfony/phpunit-bridge": "^3.4 || ^4.0"
    },
    "replace": {
        "ruwork/admin-bundle": "*",
        "ruwork/annotation-tools": "*",
        "ruwork/api-bundle": "*",
        "ruwork/bundle-test": "*",
        "ruwork/doctrine-behaviors-bundle": "*",
        "ruwork/doctrine-postgresql-bundle": "*",
        "ruwork/feature-bundle": "*",
        "ruwork/filter-bundle": "*",
        "ruwork/frujax-bundle": "*",
        "ruwork/manual-auth-bundle": "*",
        "ruwork/paginator": "*",
        "ruwork/paginator-bundle": "*",
        "ruwork/reform": "*",
        "ruwork/reform-bundle": "*",
        "ruwork/route-optional-prefix": "*",
        "ruwork/routing-tools-bundle": "*",
        "ruwork/runet-id-basket-bundle": "*",
        "ruwork/runet-id-bundle": "*",
        "ruwork/ruwork-bundle": "*",
        "ruwork/synchronizer": "*",
        "ruwork/synchronizer-bundle": "*",
        "ruwork/template-i18n-bundle": "*",
        "ruwork/upload-bundle": "*",
        "ruwork/wizard-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ruwork\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "merge-plugin": {
            "require": "src/*/composer.json"
        }
    }
}
