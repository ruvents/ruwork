{
    "name": "ruwork/object-store",
    "description": "Ruwork Object Store",
    "type": "symfony-bundle",
    "keywords": [
        "ruwork"
    ],
    "homepage": "https://ruvents.github.io/ruwork/",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Udaltsov",
            "email": "udaltsov@ruvents.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "psr/container": "^1.0",
        "symfony/options-resolver": "^3.4 || ^4.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-foundation": "^3.4 || ^4.0",
        "symfony/serializer": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\ObjectStore\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.12-dev"
        }
    }
}
